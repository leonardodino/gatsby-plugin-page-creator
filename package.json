{
  "name": "@leonardodino/gatsby-plugin-page-creator",
  "version": "0.0.1",
  "description": "Gatsby plugin that automatically creates pages from React components in specified directories",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leonardodino/gatsby-plugin-page-creator.git"
  },
  "homepage": "https://github.com/leonardodino/gatsby-plugin-page-creator#readme",
  "bugs": "https://github.com/leonardodino/gatsby-plugin-page-creator/issues",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "contributors": [
    "Steven Natera <tektekpush@gmail.com> (https://twitter.com/stevennatera)"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "bluebird": "^3.5.0",
    "chokidar": "^1.7.0",
    "fs-exists-cached": "^1.0.0",
    "glob": "^7.1.1",
    "lodash": "^4.17.10",
    "micromatch": "^3.1.10",
    "parse-filepath": "^1.0.1",
    "slash": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.0.5",
    "jest": "^23.6.0"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  }
}
